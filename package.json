{
  "name": "m2",
  "version": "1.0.0",
  "description": "NodeJS Mentoring",
  "main": "index.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "nodemon ./src/index.ts",
    "prod": "npm run lint && tsc && node ./dist/index.js",
    "lint": "eslint . --ext .ts",
    "test": "jest --config ./src/jest.config.ts",
    "test:watch": "npm run test -- --watch --coverage",
    "jest": "./node_modules/.bin/jest",
    "sequelize": "./node_modules/.bin/sequelize",
    "sequelize:migrate": "sequelize db:migrate --env development",
    "sequelize:migrate:undo": "sequelize db:migrate:undo --env development",
    "sequelize:seed": "sequelize db:seed:all",
    "sequelize:seed:undo": "sequelize db:seed:undo:all",
    "populate-db": "npm run sequelize:migrate && npm run sequelize:seed",
    "clear-db": "npm run sequelize:migrate:undo && npm run sequelize:seed:undo"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.7",
    "@types/hapi__joi": "^17.1.4",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.6.0",
    "@types/pg": "^7.14.4",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "sequelize-mock": "^0.10.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-joi-validation": "^4.0.4-beta.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "winston": "^3.3.3"
  }
}
